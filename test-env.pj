make_tree = f(arr) {
	create_tree = f(arr, low, high) {
		if(low > high) return null;
		mid = (low + high) / 2

		return {
			value: arr[mid],
			left: create_tree(arr, low, mid - 1)
			right: create_tree(arr, mid + 1, high),
		}
	}

	inorder = f(root) {
		if(root == null) return;
		
		inorder(root.left);
		print(root.value);
		inorder(root.right);
	}
	
	tree = create_tree(A, 0, len(A) - 1);

	return {
		view: f() => print(tree),
		inorder: f() => inorder(tree)
	}
}

A = [1,2,3,4,5,6,7];
tree = make_tree(A)

tree.inorder();
tree.view();