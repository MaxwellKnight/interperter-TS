create_tree = f(arr, low, high) {
	if(low > high) return null;
	mid = (low + high) / 2

	return {
		value: arr[mid],
		right: create_tree(arr, mid + 1, high),
		left: create_tree(arr, low, mid - 1)
	};
}

inorder = f(root) {
	if(root == null) return;
	
	inorder(root.left);
	print(root.value);
	inorder(root.right)
}

A = [1,2,3,4,5,6];
tree = create_tree(A, 0, len(A) - 1)
inorder(tree)